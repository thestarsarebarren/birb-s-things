  <script>
    function sendMessage() {
      var request = new XMLHttpRequest();
      request.open("POST", "https://discordapp.com/api/webhooks/731154932836597790/GV_88Q2z002f9jo6jD4sTzIqjFjGsHSqICdazD7JZVvmaQQCax80nZ0Gs3NEuSF6Bhpc/github");

      request.setRequestHeader('Content-type', 'application/json');

      var params = {
        username: "mistake",
        avatar_url: "",
        content: "angel didn't know the info category channels had publicly available webhook access so here's this, hoping this works"
      }

      request.send(JSON.stringify(params));
    }
  </script>
